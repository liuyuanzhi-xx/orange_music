/* components/loading/loading.wxss */
.fixed {
    position: fixed;
}

.absolute {
    position: absolute;
}

.loadingContainer {
    width: 100rpx;
    height: 100rpx;
    overflow: hidden;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -70%);
}


.loadingContainer .img {
    width: 100%;
    height: 100%;
}